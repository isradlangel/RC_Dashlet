Report Name: {{report_name}}
<table id='{{report_id}}' class='stripe row-border order-column' cellspacing='0' width='100%'>
    <thead>
        <tr>
            {{#each header}}
                <th width="{{width}}%">{{text}}</th>
            {{/each}}
        </tr>
    </thead>
    <tbody>
    {{#each body}}
        <tr>
        {{#each this}}
            <td>{{this}}</td>
        {{/each}}
        </tr>
    {{/each}}
    </tbody>
</table>